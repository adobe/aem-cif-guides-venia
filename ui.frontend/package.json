{
  "name": "aem-maven-archetype",
  "version": "1.0.0",
  "description": "Generates an AEM Frontend project with Webpack",
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/aem-project-archetype"
  },
  "private": true,
  "main": "src/main/site/main.js",
  "license": "SEE LICENSE IN LICENSE.txt",
  "scripts": {
    "dev": "npm run i18n:extract-compile && webpack -d --env dev --config ./webpack.dev.js && clientlib --verbose",
    "prod": "npm run i18n:extract-compile && webpack -p --config ./webpack.prod.js && clientlib --verbose",
    "start": "npm run i18n:extract-compile && webpack-dev-server --open --config ./webpack.dev.js",
    "lint": "eslint 'src/**/*.js'",
    "lint:fix": "eslint --fix 'src/**/*.js'",
    "test": "npm run i18n:extract-compile && npm run lint && npm run prettier:check && jest --coverage",
    "ci": "npm run i18n:extract-compile && npm run lint && npm run prettier:check && jest --ci --runInBand --coverage",
    "test:watch": "npm run i18n:extract-compile && npm run lint && jest --watch",
    "prettier:check": "prettier --check '{src,test}/**/*.{js,css}' --config ./.prettierrc",
    "prettier:fix": "prettier --write '{src,test}/**/*.{js,css}' --config ./.prettierrc",
    "eslint:check": "eslint 'src/**/*.js'",
    "eslint:fix": "eslint --fix 'src/**/*.js'",
    "i18n:extract-compile": "formatjs extract --out-file i18n/__temp.json 'src/**/*.js' && formatjs compile --ast --out-file i18n/en.json i18n/__temp.json"
  },
